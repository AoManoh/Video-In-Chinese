# AIAdaptor 服务配置示例
# 复制此文件为 .env 并填写实际值

# gRPC 服务端口
AI_ADAPTOR_GRPC_PORT=50053

# API 密钥加密密钥（必填，32 字节十六进制字符串，64 个字符）
# 生成方法: openssl rand -hex 32
API_KEY_ENCRYPTION_SECRET=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# 配置缓存配置
CONFIG_CACHE_TTL=600                 # 配置缓存过期时间（秒，默认 10 分钟）

# 音色管理配置
VOICE_CACHE_TTL=0                    # 音色缓存过期时间（秒，0 表示不过期）
VOICE_REGISTER_TIMEOUT=60            # 音色注册超时时间（秒）
VOICE_REGISTER_RETRY=3               # 音色注册失败重试次数
VOICE_REGISTER_RETRY_INTERVAL=5      # 音色注册重试间隔（秒）

# API 调用配置
API_CALL_TIMEOUT=30                  # 外部 API 调用超时时间（秒）
API_CALL_RETRY=2                     # 外部 API 调用失败重试次数
API_CALL_RETRY_INTERVAL=2            # 外部 API 调用重试间隔（秒）

# 日志配置
LOG_LEVEL=info                       # 日志级别（debug, info, warn, error）

